    </owl:Class>
    


    <!-- http://www.semanticweb.org/linau/ontologies/2025/4/untitled-ontology-10#Midfielder -->

    <owl:Class rdf:about="http://www.semanticweb.org/linau/ontologies/2025/4/untitled-ontology-10#Midfielder">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/linau/ontologies/2025/4/untitled-ontology-10#Position"/>
        <rdfs:comment>Milieux de terrain</rdfs:comment>
    </owl:Class>
    


